#include<stdio.h>
int main()
{
  int marks1,marks2,marks3,marks4,marks5,sum,per;
  
  printf("enter marks1");
  scanf("%d",&marks1);

  printf("enter marks2");
  scanf("%d",&marks2);

  printf("enter marks3");
  scanf("%d",&marks3);

  printf("enter marks4");
  scanf("%d",&marks4);

  printf("enter marks5");
  scanf("%d",&marks5);

  sum=marks1+marks2+marks3+marks4+marks5;
  per=sum/5;

  printf("sum =%d",&sum);
  printf("percentage=%d",&per);
  
  






  
  return 0;
}
